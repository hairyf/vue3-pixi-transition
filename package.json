{
  "name": "vue3-pixi-transition",
  "type": "module",
  "version": "0.1.3",
  "packageManager": "pnpm@8.5.0",
  "description": "Register global imports on demand for Vite and Webpack",
  "license": "MIT",
  "homepage": "https://github.com/hairyf/vue3-pixi-transition#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hairyf/vue3-pixi-transition.git"
  },
  "bugs": {
    "url": "https://github.com/hairyf/vue3-pixi-transition/issues"
  },
  "keywords": [
    "pixi.js",
    "vue",
    "transition"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./compiler": {
      "types": "./dist/compiler.d.ts",
      "require": "./dist/compiler.cjs",
      "import": "./dist/compiler.js"
    },
    "./global": {
      "types": "./dist/global.d.ts",
      "require": "./dist/global.cjs",
      "import": "./dist/global.js"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist",
    "global.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "lint": "eslint .",
    "play": "npm -C playground run dev",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish",
    "test": "vitest"
  },
  "dependencies": {
    "@antfu/utils": "^0.7.2",
    "pixi.js": "^7.2.4",
    "vue-demi": "^0.14.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.43.0",
    "@types/node": "^18.11.13",
    "bumpp": "^9.0.0",
    "eslint": "^8.29.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.0",
    "vitest": "^0.31.0"
  }
}
